<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.8.xsd">

  <changeSet id="00000000000001" author="rbobko" context="dev">
    <loadData tableName="quote"
      file="db/data/quote.csv"
      separator=";">
      <column name="id" type="numeric"/>
      <column name="message" type="string"/>
      <column name="author" type="string"/>
      <column name="date" type="date"/>
    </loadData>

    <loadData tableName="status"
      file="db/data/status.csv"
      separator=";">
      <column name="id" type="numeric"/>
      <column name="title" type="string"/>
      <column name="message" type="string"/>
      <column name="icon_url" type="string"/>
      <column name="button_text" type="string"/>
    </loadData>
  </changeSet>

</databaseChangeLog>
